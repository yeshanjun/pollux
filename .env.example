# Logging verbosity for tracing (e.g. error, warn, info, debug, trace).
LOGLEVEL=info

# Required Nexus API key used to authorize inbound requests.
NEXUS_KEY=pwd

# JSON array of Gemini models to treat as "big" models.
BIGMODEL_LIST=["gemini-2.5-pro"]

# Optional directory containing credential JSON files to preload.
CRED_PATH="./credentials"

# Max number of concurrent OAuth refresh jobs processed by the worker.
REFRESH_CONCURRENCY=10

# Allow reqwest clients to use HTTP/2 multiplexing. Leave false to force HTTP/1.
ENABLE_MULTIPLEXING=false

# Optional outbound HTTP proxy (scheme://user:pass@host:port). Remove if unused.
PROXY="http://username:password@proxy.company.com:8080"
